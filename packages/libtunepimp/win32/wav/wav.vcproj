<?xml version="1.0" encoding = "Windows-1252"?>
<VisualStudioProject
	ProjectType="Visual C++"
	Version="7.00"
	Name="wav plugin"
	ProjectGUID="{D45A4315-EE9D-40F0-87FE-F033822AFBA1}"
