				Name="VCLinkerTool"
				OutputFile="../../plugins/wav.tpp"
				LinkIncremental="2"
				ModuleDefinitionFile="wav.def"
				GenerateDebugInformation="TRUE"
				ProgramDatabaseFile="$(OutDir)/wav.pdb"
				SubSystem="2"
				ImportLibrary="$(OutDir)/wav.lib"
				TargetMachine="1"/>
			<Tool
				Name="VCMIDLTool"/>
			<Tool
				Name="VCPostBuildEventTool"
				CommandLine="mkdir ..\plugins
copy $(Targetpath) ..\plugins\wav.tpp
"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="VCResourceCompilerTool"/>
			<Tool
				Name="VCWebServiceProxyGeneratorTool"/>
			<Tool
				Name="VCWebDeploymentTool"/>
		</Configuration>
		<Configuration
			Name="Release|Win32"
			OutputDirectory="Release"
			IntermediateDirectory="Release"
			ConfigurationType="2"
			CharacterSet="2">
