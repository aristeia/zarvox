TPAlbumStatus					T_IV
TPAlbumType					T_IV
TPError						T_IV
TPFileStatus					T_IV
TPResultType					T_IV
tunepimp_t					TUNEPIMP_T
track_t						TRACK_T
metadata_t *					METADATA_T
const metadata_t *				METADATA_T
const char *		T_PV

INPUT
TUNEPIMP_T
	$var = (tunepimp_t) SvIV( (SV*) SvRV($arg) );
TRACK_T
	$var = (track_t) SvIV( (SV*) SvRV($arg) );
METADATA_T
	$var = (metadata_t *) SvIV( (SV*) SvRV($arg) );

OUTPUT
TUNEPIMP_T
	sv_setref_iv($arg, "MusicBrainz::Tunepimp::tunepimp", (I32) $var);
TRACK_T
	sv_setref_iv($arg, "MusicBrainz::Tunepimp::track", (I32) $var);
METADATA_T
	sv_setref_iv($arg, "MusicBrainz::Tunepimp::metadata", (I32) $var);

